package fiap.com.jogo;


public class Jogo {

	public static void main(String[] args) {
		Guerreiro gutts = new Guerreiro();
		Mago merlyn = new Mago();
		Dragao banguela = new Dragao();
		BolaDeFogo fireBoll = new BolaDeFogo();
		
		Fase fase = new Fase();
		
		
		fase.carregar(gutts);
		fase.carregar(merlyn);
		fase.carregar(banguela);
		fase.carregar(fireBoll);
		
		
	}
	
}
